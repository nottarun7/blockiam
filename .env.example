# Ethereum Node RPC URL (Ganache, Infura, Alchemy, etc.)
RPC_URL=http://127.0.0.1:7545

# Private key of the account to use for transactions (without 0x prefix)
PRIVATE_KEY=your_private_key_here

# Address of the deployed IAM.sol contract
CONTRACT_ADDRESS=your_contract_address_here

# Chain ID (1337 for Ganache, 1 for mainnet, 11155111 for Sepolia testnet)
CHAIN_ID=1337
